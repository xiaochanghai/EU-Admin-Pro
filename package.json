{
  "name": "eu-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "eu-admin open source management system",
  "author": {
    "name": "xiaochanghai",
    "email": "xiaochanghai@foxmail.com",
    "url": "https://github.com/xiaochanghai"
  },
  "license": "MIT",
  "homepage": "https://github.com/xiaochanghai/EU-Admin-Pro",
  "repository": {
    "type": "git",
    "url": "git@github.com:xiaochanghai/EU-Admin-Pro.git"
  },
  "bugs": {
    "url": "https://github.com/xiaochanghai/EU-Admin-Pro/issues"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build": "tsc && vite build --mode production",
    "type:check": "tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@ant-design/happy-work-theme": "^1.0.1",
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@eslint/compat": "^2.0.0",
    "@reduxjs/toolkit": "^2.11.0",
    "ahooks": "^3.9.6",
    "antd": "^6.0.0",
    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "echarts-liquidfill": "^3.1.0",
    "i18next": "^25.6.3",
    "keepalive-for-react": "^4.0.3",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-countup": "^6.5.3",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.17.16",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.5",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "cz-git": "^1.11.0",
    "czg": "^1.11.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.7.3",
    "vite": "^7.1.5",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
